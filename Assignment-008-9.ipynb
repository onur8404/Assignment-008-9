   "source": [
    "a = str(input(\"enter the sentence:\"))\n",
    "def char_frequency(a):\n",
    "    dict = {}\n",
    "    for n in a:\n",
    "        keys = dict.keys()\n",
    "        if n in keys:\n",
    "            dict[n] += 1\n",
    "        else:\n",
    "            dict[n] = 1\n",
    "    return dict\n",
    "print(char_frequency(a))"
   ]
